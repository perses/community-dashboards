                    "query": "histogram_quantile(\n  0.99,\n  sum by (le) (\n    tempo_request_duration_seconds_bucket{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"tempo_api_.*\"}\n  )\n)"
                  }
                }
              }
            }
          ]
        }
      },
      "3_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Unhealthy pods",
            "description": "Shows the status of Tempo deployment"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "decimal"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 1,
                "palette": {
                  "mode": "auto"
                },
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "  kube_deployment_status_replicas_unavailable{cluster=~\"$cluster\",deployment=~\".*(cortex-gw|distributor|ingester|query-frontend|querier|compactor|metrics-generator).*\",namespace=~\"$namespace\"}\n\u003e\n  0",
                    "seriesNameFormat": "{{deployment}}"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "    kube_statefulset_status_replicas_current{cluster=~\"$cluster\",namespace=~\"$namespace\",statefulset=~\".*(cortex-gw|distributor|ingester|query-frontend|querier|compactor|metrics-generator).*\"}\n  -\n    kube_statefulset_status_replicas_ready{cluster=~\"$cluster\",namespace=~\"$namespace\",statefulset=~\".*(cortex-gw|distributor|ingester|query-frontend|querier|compactor|metrics-generator).*\"}\n\u003e\n  0",
                    "seriesNameFormat": "{{statefulset}}"
                  }
                }
              }
            }
          ]
        }
      },
      "3_1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Pods count per version",
            "description": "Table with the count of tempo pods by version"
          },
          "plugin": {
            "kind": "Table",
            "spec": {
              "columnSettings": [
                {
                  "name": "container",
                  "header": "Container",
                  "format": {
                    "unit": ""
                  },
                  "align": "left"
                },
                {
                  "name": "version",
                  "header": "Version",
                  "format": {
                    "unit": ""
                  },
                  "align": "left"
                },
                {
                  "name": "value",
                  "header": "number of pods",
                  "format": {
                    "unit": ""
                  },
                  "align": "right"
                },
                {
                  "name": "timestamp",
                  "format": {
                    "unit": ""
                  },
                  "hide": true
                }
              ],
              "transforms": [
                {
                  "kind": "MergeSeries",
                  "spec": {}
                }
              ]
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "count by (container, version) (\n  label_replace(\n    kube_pod_container_info{cluster=~\"$cluster\",container=~\".*(cortex-gw|distributor|ingester|query-frontend|querier|compactor|metrics-generator).*\",namespace=~\"$namespace\"},\n    \"version\",\n    \"$1\",\n    \"image\",\n    \".*:(.+)-.*\"\n  )\n)"
                  }
                }
              }
            }
          ]
        }
      },
      "3_2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Latency vs 24h ago",
            "description": "Shows the 99th quantile latency of Tempo Read and Writes."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "list",
                "size": "small"
              },
              "yAxis": {
                "format": {
                  "unit": "milliseconds"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 0.5,
                "palette": {
                  "mode": "auto"
                }
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "  1\n-\n  (\n      avg_over_time(\n        histogram_quantile(\n          0.99,\n          sum by (le) (\n            tempo_request_duration_seconds_bucket{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"(opentelemetry_proto_collector_trace_v1_traceservice_export|otlp_v1_traces)\"} offset 1d\n          )\n        )[1h:]\n      )\n    /\n      avg_over_time(\n        histogram_quantile(\n          0.99,\n          sum by (le) (\n            tempo_request_duration_seconds_bucket{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"(opentelemetry_proto_collector_trace_v1_traceservice_export|otlp_v1_traces)\"}\n          )\n        )[1h:]\n      )\n  )",
                    "seriesNameFormat": "writes"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "  1\n-\n  (\n      avg_over_time(\n        histogram_quantile(\n          0.99,\n          sum by (le) (\n            tempo_request_duration_seconds_bucket{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"tempo_api_.*\"} offset 1d\n          )\n        )[1h:]\n      )\n    /\n      avg_over_time(\n        histogram_quantile(\n          0.99,\n          sum by (le) (\n            tempo_request_duration_seconds_bucket{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"tempo_api_.*\"}\n          )\n        )[1h:]\n      )\n  )",
                    "seriesNameFormat": "reads"
                  }
                }
              }
            }
          ]
        }
      }
    },
