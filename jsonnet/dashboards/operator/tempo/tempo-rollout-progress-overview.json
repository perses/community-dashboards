            "name": "Writes 99th latency",
            "description": "Shows the 99th quantile latency for Tempo"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "last",
              "format": {
                "unit": "seconds"
              },
              "thresholds": {
                "mode": "absolute",
                "defaultColor": "green",
                "steps": [
                  {
                    "value": 1,
                    "color": "orange"
                  },
