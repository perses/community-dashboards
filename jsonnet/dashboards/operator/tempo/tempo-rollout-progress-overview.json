                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "  sum(\n    rate(\n      tempo_request_duration_seconds_count{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"tempo_api_.*\",status_code=~\"2.+\"}[$__rate_interval]\n    )\n  )\n/\n  sum(\n    rate(\n      tempo_request_duration_seconds_count{cluster=~\"$cluster\",job=~\"($namespace)/cortex-gw(-internal)?\",route=~\"tempo_api_.*\"}[$__rate_interval]\n    )\n  )"
                  }
                }
              }
            }
          ]
        }
      },
