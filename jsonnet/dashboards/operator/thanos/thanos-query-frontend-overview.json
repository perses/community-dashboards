                      "query": "sum by (namespace, job, tripperware) (\n  rate(querier_cache_misses_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval])\n)",
                      "seriesNameFormat": "{{tripperware}} - {{job}} {{namespace}}"
                    }
